mulpoi<-function(mod1,mod2){
loglik   <- mod1$logLik[1] + mod2$logLik[3]
AIC      <- mod2$AIC[1]    + mod2$AIC[3]
AICC     <- mod2$AICC[1]  + mod2$AICC[3]
BIC      <- mod2$BIC[1]    + mod2$BIC[3]
Deviance <- mod2$Deviance[1]    + mod2$Deviance[3]
nvar<-mod1$nvar + mod2$nvar2
cat("No. of Estimates = ",nvar,"\n")
cat("Loglik = ",loglik,"\n")
cat("AIC  = ",AIC,"\n")
cat("AICC = ",AICC,"\n")
cat("BIC  = ",BIC,"\n")
cat("Deviance = ",Deviance,"\n")
cat("phi1 = ",mod1$Phi1,"\n")
cat("phi2 = ",mod1$Phi2,"\n")
cat("phi3 = ",mod2$Phi2,"\n")
tobs<-mod1$N
co1   <- mod1$coeff
co2   <- mod2$coeff[(mod2$nvar1+1):mod2$nvar,]
levels(co2$Var.Names)[levels(co2$Var.Names)=="Y2:Constant"] <- "Y3:Constant"
icob1 <- rbind(co1,co2)
icobTp <- 2*(1-pt(abs(icob1$t.value),tobs-nvar))
icob1$p_value <- icobTp
## Dispersion adjuested s.d and p-values
icobsed <-icob1$Adj.s.e
icobTd  <- icob1$Coeff/icobsed
icobTpd <- 2*(1-pt(abs(icobTd),tobs-2*nvar))
icob1$p_value.1   <- icobTpd
colnames(icob1)<-c("Variables","Coeff.","s.e","t.value","p.value","Adj.s.e","Ad.p.value")
myres <- list("Coeff"=icob1,"Loglik"=loglik,"AIC"=AIC,"AICC"=AICC,
"BIC"=BIC,"Deviance"=Deviance,"N"=tobs,"Nvar"=nvar, "Phi1"=mod1$Phi1,
"Phi2"=mod1$Phi2,"Phi3"=mod2$Phi2)
return(myres)
}
